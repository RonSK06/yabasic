#!./yabasic


if (or(0b10,0b01)<>0b11) error "1"

if (or(0b110,0b101)<>0b111) error "2"

if (and(0b10,0b01)<>0b00) error "3"

if (and(0b110,0b101)<>0b100) error "4"

if (eor(0b110,0b101)<>0b11) error "5"

if (eor(0b10,0b101)<>0b111) error "6"

if (not(0b11)<>0b111110) error "7"

if (shl(0b11,2)<>0b1100) error "8"

if (shr(0b1100,2)<>0b11) error "9"

if (bitnot(0b1001110)<>0b11111111111111111111111110110001) error "10"

exit 0

